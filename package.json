{
  "name": "@webpros/360-api-client",
  "version": "0.0.6",
  "description": "JS API Client to connect to the Plesk 360 Monitoring Engine",
  "author": "Nils Langner (nils.langner@webpros.com)",
  "contributors": [
    "Sascha Fuchs <sascha.fuchs@webpros.com>"
  ],
  "license": "MIT",
  "main": "./dist/360ApiClient.js",
  "module": "./dist/360ApiClient.esm.js",
  "typings": "./dist/types/360ApiClient.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/leankoala-gmbh/leankoala-client-typescript.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "test": "jest",
    "generateClient": "esbuild src/360ApiClient.ts --bundle --minify --format=esm --target=es2020 --outfile=dist/360ApiClient.js",
    "generateExtraFiles": "esbuild src/Connection/RefreshTokenInvalidError.ts src/Connection/BadRequestError.ts src/Repository/Constants/Marketplace.ts --minify --format=esm --target=esnext --outdir=dist/",
    "generateTypes": "tsc --emitDeclarationOnly --declaration --project tsconfig.json",
    "oldPublish": "npm run generateTypes && npm run generateExtraFiles && npm run generateClient",
    "prePublish": "node esbuild.js",
    "lint": "eslint --cache --max-warnings 0 --fix --ext .ts .",
    "typedoc": "typedoc --out docs src/360ApiClient.ts"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jwt-decode": "^2.2.0"
  },
  "devDependencies": {
    "esbuild-plugin-import-glob": "^0.1.1",
    "npm-dts": "^1.3.11",
    "tslib": "2.4.0",
    "@babel/preset-env": "^7.13.10",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "moxios": "^0.4.0",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "4.6.4"
  }
}
